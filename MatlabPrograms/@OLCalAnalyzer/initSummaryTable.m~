function initSummaryTable(obj)

    obj.summaryTableFigure = figure('Position',[440 500 461 146]);
    set(obj.summaryTableFigure, 'MenuBar', 'none');
    % nan data
    d = [nan nan; nan nan];

    % Create the column and row names in cell arrays 
    times = {'Before calibration','After calibration'};
    spdNames = {'<html><h4>darkMeas</h4></html>','halfOnMeas'};

    % Create the uitable
    obj.summaryTable = uitable(obj.summaryTableFigure, ...
        'Data',d,...
        'ColumnName', times,... 
        'RowName', spdNames);
    obj.summaryTable.FontName = 'Menlo';
    obj.summaryTable.FontSize = 12;
    
    % Set width and height
    obj.summaryTable.Position(3) = obj.summaryTable.Extent(3);
    obj.summaryTable.Position(4) = obj.summaryTable.Extent(4);
end

